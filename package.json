{
  "name": "i18ncraft",
  "version": "0.1.0-beta.2",
  "packageManager": "pnpm@10.13.1",
  "description": "ðŸš€ Professional internationalization automation tool with intelligent batch text transformation and enterprise-grade error handling.",
  "author": "exwer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/exwer",
  "homepage": "https://github.com/exwer/i18ncraft#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exwer/i18ncraft.git"
  },
  "bugs": {
    "url": "https://github.com/exwer/i18ncraft/issues"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "localization",
    "vue",
    "cli",
    "automation",
    "transformation",
    "typescript"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "i18ncraft": "./dist/i18ncraft.cjs"
  },
  "files": [
    "dist",
    "README.md",
    "README.zh.md"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "prepublishOnly": "npm run build && npm run test",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/core": "^7.17.10",
