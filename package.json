{
  "name": "vite-plugin-vue3-auto-i18n",
  "version": "0.1.2-beta.1",
  "packageManager": "pnpm@6.32.3",
  "description": "Auto import Vue-i18n in every vue sfc files and auto replace i18n words in vue sfc files.",
  "keywords": [
    "vue3",
    "i18n",
    "auto",
    "vue-i18n",
    "vite plugin"
  ],
  "homepage": "https://github.com/love-js/vite-plugin-vue3-auto-i18n#readme",
  "bugs": {
    "url": "https://github.com/love-js/vite-plugin-vue3-auto-i18n/issues"
  },
  "license": "MIT",
  "author": "love-JS <ss.lbw.ss@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/love-js/vite-plugin-vue3-auto-i18n.git"
  },
  "funding": "https://github.com/sponsors/love-js",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "vite",
    "vite:build": "vite build",
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "esno": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@antfu/ni": "^25.0.0",
    "@babel/types": "^7.17.10",
    "@types/babel__core": "^7.1.19",
    "@types/node": "^24.0.14",
    "@vitejs/plugin-vue": "^6.0.0",
    "bumpp": "^10.2.0",
    "eslint": "^9.31.0",
    "esno": "^4.8.0",
    "pnpm": "^10.13.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vite": "^7.0.5",
    "vite-plugin-inspect": "^11.3.0",
    "vitest": "^3.2.4",
    "vue": "^3.2.33",
    "vue-i18n": "^11.1.10"
  },
  "dependencies": {
    "@babel/core": "^7.17.10",
    "@vue/compiler-dom": "^3.5.17",
    "@vue/compiler-sfc": "^3.2.33"
  },
  "peerDependencies": {
    "vue-i18n": "^11"
  }
}
