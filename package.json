    "recast": "^0.23.11",
    "unplugin": "^2.3.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@antfu/ni": "^25.0.0",
    "@babel/types": "^7.28.1",
    "@swc/core": "^1.13.2",
    "@types/babel__core": "^7.1.19",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^24.0.14",
    "@vitejs/plugin-vue": "^6.0.0",
    "bumpp": "^10.2.0",
    "eslint": "^9.31.0",
    "esno": "^4.8.0",
    "pnpm": "^10.13.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.7.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vitest": "^3.2.4",
