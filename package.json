{
  "name": "i18nCraft",
  "version": "0.0.1",
  "packageManager": "pnpm@10.13.1",
  "description": "ðŸš€ i18nCraft - The modern CLI toolkit for batch internationalization (i18n) refactoring and automation. Effortlessly scan, transform, and manage i18n keys in Vue and beyond.",
  "keywords": [
    "vue3",
    "react",
    "i18n",
    "cli",
    "auto",
    "vue-i18n",
    "react-i18n"
  ],
  "homepage": "https://github.com/exwer/i18ncraft#readme",
  "bugs": {
    "url": "https://github.com/exwer/i18ncraft/issues"
  },
  "license": "MIT",
  "author": "exwer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exwer/i18nCraft.git"
  },
  "funding": "https://github.com/sponsors/exwer",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint .",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "esno": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@antfu/ni": "^25.0.0",
    "@babel/types": "^7.17.10",
    "@types/babel__core": "^7.1.19",
    "@types/node": "^24.0.14",
    "@vitejs/plugin-vue": "^6.0.0",
    "bumpp": "^10.2.0",
    "eslint": "^9.31.0",
    "esno": "^4.8.0",
    "pnpm": "^10.13.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^5.8.3",
    "vue": "^3.2.33",
    "vue-i18n": "^11.1.10"
  },
  "dependencies": {
    "@babel/core": "^7.17.10",
    "@vue/compiler-dom": "^3.5.17",
    "@vue/compiler-sfc": "^3.2.33"
